<% provide(:title, "Home") %>
<% if logged_in? %>
    <div class="col s6 m4">
        <%= render 'shared/feed' %>
    </div>
<% else %>
    <div class="center">
        <%= link_to image_tag("MyImage.png", alt: "My image", width: '300', height: '300', class: "card-panel hoverable"),
            'http://rubyonrails.org/' %>
    </div>
    <div class="center">
        <%= link_to "Sign up now!", signup_path, class: "btn btn-lg btn-primary" %>
    </div>
<% end %>