<% if @feed_items.any? %>
  <div class='row'>
      <% @feed_items.each do |t| %>
        <% if t.activated == true %>
        <div class='col s12 m6 l4'>
          <div class= 'card'>
            <div class='card-image waves-effect waves-block waves-light'>
              <%= link_to image_tag(t.picture.gallery_image.url ), painting_path(t.id) %>
            </div>
          </div>
        </div>
        <% end %>
      <% end %>
    </div>
  </div>
  <%#= will_paginate @feed_items %>
<% end %>